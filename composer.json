{
    "name": "yesworld/phalcon5-php8-docker",
    "version": "0.0.1",
    "description": "Simple starter Web-project for PHP Phalcon",
    "keywords": ["Php", "phalcon", "mysql", "docker"],
    "require": {
        "php": "~8.1",
        "ext-psr": "1.1.0",
        "ext-phalcon": ">=5.1.4",
        "ext-pdo": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "phpunit/phpunit": "^10",
        "symfony/http-client": "^6.2"
    },
    "authors": [
        {
            "name": "Damir Ahmedshin",
            "email": "a.yesworld@gmail.com"
        },
        {
            "name": "davidbayra",
            "email": "davidbayra@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "type": "library",
    "license": "MIT"
}
